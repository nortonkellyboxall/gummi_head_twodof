<launch>
    <!-- Start tilt joint controller -->
    <rosparam file="$(find two_dof_head)/config/tilt.yaml" command="load"/>
    <node name="tilt_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port pan_tilt_port
                joint3_controller
                joint4_controller"
          output="screen"/>
</launch>
